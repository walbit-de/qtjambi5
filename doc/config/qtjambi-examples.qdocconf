project                 = QtJambi
description             = QtJambi Reference Documentation
url                     = http://doc.trolltech.com/java/4.1

outputdir               = ../html
sourcedirs              = ../src
sources.fileextensions  = "*.java *.qdoc"
exampledirs             = ../../com/trolltech/examples
imagedirs               = ../src/images

extraimages.HTML        = qt-logo \
                          trolltech-logo

alias.also                      = sa
alias.group                     = defgroup
alias.i                         = e
alias.include                   = input
alias.inheaderfile              = headerfile
alias.quotefile                 = include
alias.quotefromfile             = quotefile
alias.related                   = relates

macro.key               = "\\bold"
