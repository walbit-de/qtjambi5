<?xml version="1.0"?>
<typesystem package="org.qtjambi.qt.concurrent" default-superclass="org.qtjambi.qt.internal.QtJambiObject">
    <inject-code>
	    	org.qtjambi.qt.Utilities.loadQtLibrary("QtCore");
	    	org.qtjambi.qt.Utilities.loadQtLibrary("QtConcurrent");
		org.qtjambi.qt.Utilities.loadJambiJniLibrary("org_qtjambi_qt_core");
    </inject-code>

  <namespace-type name="QtConcurrent">
        <extra-includes>
            <include file-name="org.qtjambi.qt.core.*" location="java"/>
        </extra-includes>
    <inject-code class="java">
        <import-file name="typesystem/java/typesystem_concurrent-java.java" quote-after-line="class QtConcurrent___" quote-before-line="}// class"/>
    </inject-code>
  </namespace-type>
</typesystem>
